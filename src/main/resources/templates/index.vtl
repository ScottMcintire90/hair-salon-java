<div class="container">
  <h1 class="header">PDX Cuts</h1>
  <div class="list">
    <ul>
          <h3>Stylists</h3>
          #foreach($stylist in $stylists)
          <li><span><a href="/stylists/$stylist.getId()">$stylist.getName()</span></a></li>
          <hr class="style">
          #end
    </ul>
  </div>
  <div>
    <a href="/stylists/new">Add a Stylist</a>
  </div>
</div>
